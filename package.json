{
  "name": "thebackbone",
  "version": "1.0.0",
  "description": "Node + MongoDB Backend",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Alex Stevens",
  "scripts": {
    "start": "node ./dist/app",
    "dev": "PORT=3020 nodemon src/app.js --exec babel-node",
    "build": "rm -rf dist; babel src -d dist",
    "lint": "eslint src/ --fix",
    "test": "jest src/ --watch",
    "pretty": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "deo": "^0.0.2",
    "express": "^4.15.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "jwt-simple": "^0.5.5",
    "moment": "^2.22.2",
    "mongoose": "^5.3.12",
    "morgan": "^1.9.1",
    "ololog": "^1.1.113"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-preset-node8": "^1.1.2",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lint-staged": "^4.0.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.6",
    "regenerator-runtime": "^0.12.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma --no-semi es5 --write",
      "eslint --fix",
      "git add"
    ]
  }
}
